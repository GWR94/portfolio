{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": false,
    "outDir": "dist",
    "module": "commonjs",
    "target": "es2018",
    "lib": ["es2015", "es2017", "dom"],
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "components/*": ["src/pages/*", "server/*"]
    }
  },
  "exclude": ["node_modules/*"]
}
